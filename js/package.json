{
  "name": "lineup_widget",
  "version": "1.0.1",
  "description": "Wrapper around the LineUp.js library for multi attribute rankings",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "ipywidgets",
    "widgets"
  ],
  "homepage": "https://github.com/datavisyn/lineup_widget",
  "bugs": {
    "url": "https://github.com/datavisyn/lineup_widget/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Samuel Gratzl",
    "email": "samuel_gratzl@gmx.at"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.map",
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/datavisyn/lineup_widget"
  },
  "scripts": {
    "clean": "rimraf dist ../lineup_widget/static",
    "precompile": "npm run clean",
    "compile": "tsc --project .",
    "prebuild": "npm run compile",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\"",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.2.1",
    "lineupjs": "^3.1.1"
  },
  "devDependencies": {
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.22.1",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1",
    "url-loader": "^1.1.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "jupyterlab": {
    "extension": "lib/labplugin"
  }
}
